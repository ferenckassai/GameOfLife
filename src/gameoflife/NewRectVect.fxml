<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" onMouseClicked="#okPressed" prefHeight="180.0" prefWidth="300.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gameoflife.NewRectVectController">
    <stylesheets>
        <URL value="@newrectvect.css" />
    </stylesheets>
   <children>
      <Button id="OK" fx:id="OK" layoutX="165.0" layoutY="145.0" mnemonicParsing="false" onMouseClicked="#okPressed" text="OK" />
      <Button id="cancel" fx:id="cancel" layoutX="218.0" layoutY="144.0" mnemonicParsing="false" onMouseClicked="#cancelPressed" text="Cancel" />
      <GridPane layoutX="30.0" layoutY="12.0" prefHeight="123.0" prefWidth="249.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="268.0" minWidth="10.0" prefWidth="137.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="410.0" minWidth="10.0" prefWidth="112.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Number of  columns" />
            <Label id="noOfCols" text="Number of rows" GridPane.rowIndex="1" />
            <Label id="cellSize" text="Size of cells" GridPane.rowIndex="2" />
            <TextField id="noOfRows" fx:id="noOfRows" GridPane.columnIndex="1" />
            <TextField id="noOfCols" fx:id="noOfCols" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField id="cellSize" fx:id="cellSize" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
